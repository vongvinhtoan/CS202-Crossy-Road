{
    "fps": 60,
    "window": {
        "width": 1600,
        "height": 900,
        "title": "Game"
    },
    "hide_statistic": true,
    "loading_bar": {
        "duration": 0.5
    },
    "character_picker": {
        "animation_time": 0.1,
        "animation_dynamic": {
            "f": 2.5,
            "z": 0.75,
            "r": -0.5
        }
    },
    "keyBinding": {
        "moveUp": 22,
        "moveDown": 18,
        "moveLeft": 0,
        "moveRight": 3
    },
    "playground": {
        "bufferRange": 32,
        "tileSize": 75,
        "camera": {
            "scrollAcceleration": 0.1,
            "initialScrollSpeed": 0.5
        }
    }
}